{
  "name": "ts-in-html",
  "version": "1.0.0",
  "description": "A test widget that lets TS code run in HTML",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel only*",
    "onlycmd": "nodemon --watch src/ -e ts --exec ts-node ./src/app.ts",
    "onlybrowser": "parcel ./src/index.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
